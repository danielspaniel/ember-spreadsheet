<div class="header">
  <div class="left-column">{{headerLeft}}</div>
  <div class="right-columns">{{component headerComponent}}</div>{
</div>
<div class="spreadsheet-container">
  <tr>
    <td class="left">
      {{#ember-collection class="collection" cell-layout=(fixed-grid-layout leftWidth itemHeight) items=leftItems as |item index|}}
        {{component leftSideComponent item=item}}
      {{/ember-collection}}
    </td>
    <td class="right">
      {{#ember-collection class="collection" cell-layout=(percentage-columns-layout itemCount columns itemHeight) items=rightItems as |item index|}}
        {{component rightSideComponent item=item}}
      {{/ember-collection}}
    </td>
  </tr>
</div>