{{#ember-spreadsheet
  itemHeight=itemHeight
  headerComponent=headerComponent

  headerLeft=headerLeft
  leftWidth=leftWidth
  leftItems=leftItems

  rightItems=rightItems
  rightSideComponent=rightSideComponent

   as |hash|}}
  {{#if hash.isLeftSideItem}}
    {{left-row item=hash.item}}
  {{/if}}
{{/ember-spreadsheet}}
